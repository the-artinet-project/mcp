    "prepublishOnly": "npm run clean:all && npm i && npm run build && npm test"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.17.50",
    "eslint": "^9.25.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.31.0"
  },
