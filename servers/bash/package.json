{
  "name": "@artinet/bash-mcp",
  "version": "0.0.4",
  "description": "A Model Context Protocol (MCP) server for executing bash commands.",
  "author": "artinet",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/the-artinet-project/mcp/servers/bash"
  },
  "issues": {
    "url": "https://github.com/the-artinet-project/mcp/issues"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "bash-mcp": "./dist/bin/stdio.js"
  },
  "keywords": [
    "artinet",
    "agent2agent",
    "modelcontextprotocol",
    "mcp"
  ],
  "type": "module",
  "main": "./dist/src/index.js",
